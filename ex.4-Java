public class Exercicio4 {
    public static char verificarNumero(int numero) {
        if (numero > 0) {
            return 'P';
        } else {
            return 'N';
        }
    }

    public static void main(String[] args) {
        int valor = -5;
        System.out.println("Resultado: " + verificarNumero(valor));
    }
}
